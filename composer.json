{
    "name": "wout/laravel-sns-queue-driver",
    "description": "A package that adds support for SNS as a queue driver for Laravel. Handy for microservice messaging",
    "require": {
        "php": "^8.2",
        "laravel/laravel": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Wout\\LaravelSnsQueueDriver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wout\\LaravelSnsQueueDriver\\LaravelSnsQueueDriverServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "WouterLagerwerf",
            "email": "43637154+WouterLagerwerf@users.noreply.github.com"
        }
    ]
}
