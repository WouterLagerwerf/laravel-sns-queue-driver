{
    "name": "wouterlagerwerf/laravel-sns-queue-driver",
    "description": "A package that adds support for SNS as a queue driver for Laravel. Handy for microservice messaging",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "WouterLagerwerf\\LaravelSnsQueueDriver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WouterLagerwerf\\LaravelSnsQueueDriver\\LaravelSnsQueueDriverServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Wouter Lagerwerf",
            "email": "43637154+WouterLagerwerf@users.noreply.github.com"
        }
    ],
    "license": "MIT"
}
